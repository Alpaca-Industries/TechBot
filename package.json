		}
	],
	"license": "AGPL-3.0-only",
	"dependencies": {
		"@discordjs/collection": "^0.6.0",
		"@prisma/client": "^3.14.0",
		"@sapphire/decorators": "^4.3.4",
		"@sapphire/discord-utilities": "^2.11.0",
		"@sapphire/discord.js-utilities": "^4.11.0",
		"@sapphire/fetch": "^2.3.1",
		"@sapphire/framework": "^3.0.0-next.c7c8bfd.0",
		"@sapphire/pieces": "^3.3.3",
		"@sapphire/plugin-logger": "^2.2.1",
		"@sapphire/stopwatch": "^1.4.1",
		"@sapphire/time-utilities": "^1.7.4",
		"@sapphire/type": "^2.2.4",
		"@sapphire/utilities": "^3.6.2",
		"colorette": "^2.0.16",
		"common-tags": "^1.8.2",
		"discord.js": "^13.7.0",
		"dotenv": "^16.0.1",
		"dotenv-cra": "^3.0.2",
		"reflect-metadata": "^0.1.13",
		"tslib": "^2.4.0",
		"vm2": "^3.9.9"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.5",
		"@sapphire/prettier-config": "^1.4.3",
		"@sapphire/ts-config": "^3.3.4",
		"@swc/core": "^1.2.189",
		"@types/common-tags": "^1.8.1",
		"@types/node": "^17.0.35",
		"@types/ws": "^8.5.3",
		"husky": "^8.0.1",
		"lint-staged": "^12.4.1",
		"prettier": "^2.6.2",
		"prisma": "^3.14.0",
		"tsup": "^5.12.8",
		"typescript": "^4.7.2"
	},
	"scripts": {
		"build": "tsup",
		"watch": "tsup --watch",
		"start": "node --enable-source-maps dist/index.js",
		"dev": "tsup --onSuccess  \"node --enable-source-maps  ./dist/index.js\"",
		"devw": "tsup --watch --onSuccess \"node --enable-source-maps  ./dist/index.js\"",
		"format": "prettier --write \"src/**/*.ts\"",
		"prepare": "husky install",
		"test": "tsup ./src/lib/native_extensions.ts --out-dir ./src/lib/tests/dist --silent --no-config && npx jest"
	},
	"prettier": "@sapphire/prettier-config",
	"lint-staged": {
		"*.ts": "prettier --write"
	},
	"eslintConfig": {
		"extends": "@sapphire"
	},
	"imports": {
		"#root/*": "./dist/*.js",
		"#lib/*": "./dist/lib/*.js",
		"#lib/helpers": "./dist/lib/helpers/index.js"
	},
	"tsup": {
		"clean": true,
		"dts": false,
		"entry": [
			"src/**/*.ts",
			"!src/**/*.d.ts"
		],
		"minify": true,
		"skipNodeModulesBundle": true,
		"target": "esnext",
		"tsconfig": "./src/tsconfig.json",
		"bundle": false,
		"shims": false,
		"keepNames": true,
		"splitting": false,
		"suppress": true,
		"format": [
			"esm"
		],
		"sourcemap": true
	},
	"prisma": {
		"seed": "node prisma/seed.js"
	}
}
